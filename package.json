{
	"name": "marathon-api",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"server": "NODE_ENV=dev nodemon index.js"
	},
	"eslintConfig": {
		"extends": "airbnb-base",
		"env": {
			"es6": true,
			"browser": true
		},
		"rules": {
			"brace-style": [
				"error",
				"stroustrup"
			],
			"comma-dangle": [
				"error",
				"never"
			],
			"no-unused-vars": [
				"warn"
			],
			"no-underscore-dangle": 0,
			"no-var": [
				"off"
			],
			"one-var": [
				"off"
			],
			"indent": [
				2,
				"tab"
			],
			"no-tabs": 0,
			"camelcase": 0,
			"no-console": 0
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"connect-mongo": "^3.0.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"express": "^4.17.1",
		"express-session": "^1.16.2",
		"mongoose": "^5.6.4",
		"morgan": "^1.9.1",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0"
	},
	"devDependencies": {
		"eslint": "^6.0.1",
		"eslint-config-airbnb-base": "^13.2.0",
		"eslint-plugin-import": "^2.18.0",
		"nodemon": "^1.19.1"
	}
}
